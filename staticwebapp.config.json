{
    "routes": [
        {
            "route": "/_next/static/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/static/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/*",
            "headers": {
                "Cache-Control": "public, max-age=300, s-maxage=300, must-revalidate"
            }
        }
    ]
}
